<section>
  {{#with service}}
    <div class='row'>
      <div class='col-12'>
        <div class='card text-bg-success text-center'>
          <div class='card-body'>
            <h2 class='card-title'>{{title}}</h2>
            <p class='card-text'>{{description}}</p>
            {{#each buttons}}
              <a
                href='{{link}}'
                class='btn btn-primary'
              >{{text}}</a>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  {{/with}}
</section>